import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int nst=1;
       int row=1;
       int first=0;
       int second=1;
       while(row<=n){
           int j =1;
           while(j<=nst){

               int third=first+second;
               System.out.print(first+"\t");
               j++;
               first=second;
               second=third;

           }
           nst++;
           row++;
           System.out.println();
       }

    }
}
