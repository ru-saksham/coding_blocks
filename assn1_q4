import java.util.*;

public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int a=1;

        int i=1;
        while(i<=n){


            int j =1;
            while(j<=i){


            System.out.print(a+" ");
            a++;
            j++;
            }
            
            i++;
            System.out.println();

        }
    }
}
